{
  "name": "Go to Https",
  "version": "0.0.2",
  "manifest_version": 2,
  "icons": {
    "128": "icons/icon_on_128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "go to https"
    },
  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab"
  ],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "src/content/main.js"
    ]
  }]
}
