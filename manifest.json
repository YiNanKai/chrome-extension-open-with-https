{
  "name": "Go to Https",
  "version": "0.0.2",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "go to https"
    },
  "permissions": [
    "activeTab"
  ]
}
