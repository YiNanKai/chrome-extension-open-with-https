{
  "name": "Go to Https",
  "version": "0.0.2",
  "manifest_version": 3,
  "icons": {
    "128": "icons/icon_on_128.png"
  },
  "default_locale": "en",
  "background": {
    "service_worker": "src/bg/background.js"
  },
  "action": {
    "default_title": "go to https"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "src/content/main.js"
    ]
  }]
}
